{
  "name": "nextra",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nextra",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx next build",
        "cwd": "apps/nextra"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx next dev -p 1337",
        "cwd": "apps/nextra"
      }
    }
  },
  "tags": []
}
